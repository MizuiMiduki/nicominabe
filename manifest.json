{
    "name": "ニコ見なべ",
    "version": "1.1",
    "manifest_version": 3,
    "description": "ニコニコ用ブラウザ拡張機能の詰め合わせ",
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*.nicovideo.jp/*"
            ],
            "js": [
                "/jquery-3.7.1.min.js",
                "files/dic/main.js"
            ],
            "css": [
                "files/dic/style.css"
            ]
        }
    ]
}
